{
  "name": "react-picky-date-time",
  "version": "1.7.3",
  "description": "A react component for date time picker.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwardxiao/react-picky-date-time.git"
  },
  "keywords": [
    "react",
    "picky",
    "date",
    "time",
    "picker",
    "datepicker",
    "date-picker",
    "timepicker",
    "time-picker",
    "calendar"
  ],
  "author": "Edward Xiao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edwardxiao/react-picky-date-time/issues"
  },
  "homepage": "https://github.com/edwardxiao/react-picky-date-time#readme",
  "scripts": {
    "babel": "BABEL_ENV=lib babel ./src/js/PickyDateTime --out-dir ./lib/components",
    "build": "rm -rf lib && npm run babel && npm run build_prod",
    "webpack_dev": "rm -rf dist && NODE_ENV=development ./node_modules/.bin/webpack --config ./webpack/development.config.js --progress --watch",
    "webpack_prod": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/production.config.js --progress -p",
    "build_dev": "npm run webpack_dev",
    "build_prod": "npm run webpack_prod",
    "build_umd": "./node_modules/.bin/webpack --config ./webpack/umd.global.config.js",
    "build_umd_min": "./node_modules/.bin/webpack --config ./webpack/umd.global.config.js --env build",
    "server_dev": "NODE_ENV=server node_modules/.bin/webpack-dev-server --config ./webpack/development.config.js",
    "server_prod": "NODE_ENV=server node_modules/.bin/webpack-dev-server --config ./webpack/production.config.js",
    "clean": "rimraf dist lib",
    "compile_webpack": "npm run clean && npm run build_umd && npm run build_umd_min",
    "compile": "npm run clean && npm run compile_webpack && npm run babel"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.2.0",
    "classnames": "^2.2.6",
    "coveralls": "^3.0.4",
    "css-loader": "^3.5.2",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint": "^4.18.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "inline-style-prefix-all": "^2.0.2",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.7.0",
    "open-color": "^1.7.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-simple-vars": "^5.0.2",
    "prismjs": "^1.16.0",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-update": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.9",
    "rimraf": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.0",
    "webpack": "^4.19.1",
    "webpack-assets-manifest": "^3.1.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.4"
  }
}